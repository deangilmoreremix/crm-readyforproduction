{
    "name": "vite-react-typescript-starter",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:server": "NODE_ENV=development tsx server/index.ts",
        "build": "vite build",
        "serve": "NODE_ENV=production tsx server/index.ts",
        "lint": "eslint .",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "preview": "vite preview",
        "clean-cache": "npm cache clean --force && rm -rf node_modules/.vite && rm -rf dist",
        "clean-all": "npm run clean-cache && rm -rf node_modules && npm ci",
        "rebuild": "npm run clean-cache && npm run build",
        "cache-bust": "npm run clean-all && npm run build"
    },
